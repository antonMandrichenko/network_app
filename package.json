  "scripts": {
    "start": "ts-node-dev --ignore-watch node_modules -r tsconfig-paths/register src",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "migrate:create": "yarn typeorm migration:create -n",
    "migrate:run": "yarn typeorm migration:run",
    "migrate:generate": "yarn typeorm migration:generate -n ",
    "migrate:revert": "yarn typeorm migration:revert",
    "webpack:run": "webpack --config webpack.config.js",
    "create:api": "pm2-runtime build/bundle.js"
  },
  "dependencies": {
    "@koa/cors": "^3.0.0",
    "apollo-server-koa": "^2.11.0",
    "dotenv": "^8.2.0",
