    "http-shutdown": "^1.2.2",
    "ioredis": "^4.16.0",
    "jsonwebtoken": "^9.0.0",
    "koa": "^2.11.0",
    "koa-body": "^4.1.1",
    "koa-logger": "^3.2.1",
    "koa-router": "^8.0.8",
    "koa-session": "^5.13.1",
    "lodash": "^4.17.15",
    "pg": "^7.18.2",
    "pm2": "^4.2.3",
    "reflect-metadata": "^0.1.13",
    "type-graphql": "^0.17.6",
    "typeorm": "^0.2.24"
  },
  "devDependencies": {
    "@types/graphql": "^14.5.0",
    "@types/ioredis": "^4.14.9",
    "@types/jsonwebtoken": "^8.3.8",
